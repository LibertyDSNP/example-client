{
  "name": "client",
  "homepage": "https://libertydsnp.github.io/example-client/",
  "version": "0.1.0",
  "private": false,
  "license": "Apache-2.0",
  "dependencies": {
    "@dsnp/contracts": "^0.0.0-5c945f",
    "@dsnp/parquetjs": "^1.1.0",
    "@dsnp/sdk": "^0.0.0-47f4fd",
    "@reduxjs/toolkit": "^1.5.1",
